{
  "name": "Pinter",
  "manifest_version": 2,
  "description": "Pinter",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "To pin a message on facebook"
  },
  "content_scripts": [
      {
        "matches": ["http://*/messages*", "https://*/messages*", "http://www.messenger.com/*", "https://www.messenger.com/*"],
        "js": ["jquery.min.js", "pinter.js"]
      }
    ],
  "permissions": [
    "activeTab"
  ]
}