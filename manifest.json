{
  "name": "Pinter",
  "manifest_version": 2,
  "description": "Pinter",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "1Hell yeah!"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery.min.js", "pinter.js"]
      }
    ],
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "declarativeContent"
  ]
}